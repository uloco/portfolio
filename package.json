    "eslint-plugin-react": "^7.13.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.6",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    ".src/*.{js,jsx}": "eslint",
