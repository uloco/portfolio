{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^6.3.0",
    "parallax-js": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.8.6",
    "react-particles-js": "^2.6.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "react-spring": "^8.0.27",
    "styled-components": "^4.2.1",
    "typescript": "^3.6.2"
  },
  "scripts": {
